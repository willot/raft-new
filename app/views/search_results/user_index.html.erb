<div class="standard_box">
  <% if @results.count != 0 %>
    <h1> Your Previous Searches </h1>
    <% @results.each do |result| %>
      <h3><%= result.current_city.capitalize %></h3>
      <h4>$<%= result.budget %></h4>
      <p>From: <%= result.start_at. %>
      <% result.locations.each do |location| %>
        <%= location.city %>
        <% end %>
      <br><br>
    <% end %>
  <% else %>
    <h2> You have no Searches! </h2>
  <%end%>
</div>
