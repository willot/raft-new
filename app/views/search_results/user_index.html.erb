<div class="standard_box">
  <% if @results %>
    <% @results.each do |result| %>
      <%= result.destination %><br>
      <%= result.depart_time %><br>
      <%= result.return_time %><br>
      <%= result.price%>
      <br><br>
    <% end %>
  <% else %>
    <h2> You have no Searches! </h2>
  <%end%>
</div>
