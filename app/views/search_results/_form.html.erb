<%= form_for @search_result, html: {id: "flight-search"} do |f| %>
  <%= f.label "I want to get out of" %>
  <%= f.text_field :current_city, value: @city, include_blank: true, :"data-autocomplete-path" => autocomplete_location_city_search_results_path, id: 'current-city-text-field' %><br>
  <%= f.label "by"%>
  <%= f.date_select :start_at, start_year: 2015, discard_day: true, order: [:month, :year] %><br>
  <%= f.label "with     $" %>
  <%= f.text_field :budget %><br>
  <%= f.submit "Send me somewhere" %><br><br>

    <%= flash[:error] %>
<% end %>
