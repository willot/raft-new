<%= form_for @search_result do |f| %>
  <%= f.label "I want to leave from" %>
  <%= f.text_field :current_city, value: @city, include_blank: true %><br>
  <%= f.label "I have $" %>
  <%= f.text_field :budget %><br>
  <%= f.label "I'm leaving"%>
  <%= f.date_select :start_at %><br>

  <%= f.submit "Send me somewhere" %>
<% end %>
